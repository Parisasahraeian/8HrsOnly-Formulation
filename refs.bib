	Doi = {10.1111/ele.12276},
	Eprint = {https://onlinelibrary.wiley.com/doi/pdf/10.1111/ele.12276},
	Journal = {Ecology Letters},
	Keywords = {Flowering time, life history, microbiome, phenology, plant--microbe interactions, plasticity, selection, selective agents, soil ecology},
	Number = {6},
	Pages = {717-726},
	Title = {Natural soil microbes alter flowering phenology and the intensity of selection on flowering time in a wild Arabidopsis relative},
	Url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/ele.12276},
	Volume = {17},
	Year = {2014},
	Bdsk-Url-1 = {https://onlinelibrary.wiley.com/doi/abs/10.1111/ele.12276},
