	Doi = {10.1093/bioinformatics/btn161},
	Eprint = {https://academic.oup.com/bioinformatics/article-pdf/24/13/i223/628815/btn161.pdf},
	Issn = {1367-4803},
	Journal = {Bioinformatics},
