@article{MOMMER2016209,
	Abstract = {Plant scientists have made great progress in understanding molecular mechanisms controlling root responses to nutrients of arabidopsis (Arabidopsis thaliana) plants under controlled conditions. Simultaneously, ecologists and agronomists have demonstrated that root--root interactions involve more than competition for nutrients. Here, we highlight the importance of both root exudates and soil microbes for root--root interactions, ubiquitous in natural and agricultural ecosystems. We argue that it is time to bring together the recent insights from both scientific disciplines to fully understand root functioning in the real world.},
