	Issn = {0377-2217},
	Journal = {European Journal of Operational Research},
	Keywords = {Integer programming, Lagrangean relaxation, Cutting planes, Cuts, Duality gaps, Valid inequalities, Facets},
	Number = {1},
	Pages = {216 - 223},
	Title = {Efficient cuts in Lagrangean `Relax-and-cut' schemes},
