	Issn = {1360-1385},
	Journal = {Trends in Plant Science},
	Number = {8},
	Pages = {478 - 486},
	Title = {The rhizosphere microbiome and plant health},
	Url = {http://www.sciencedirect.com/science/article/pii/S1360138512000799},
