	Issn = {0065-2164},
	Keywords = {Crop plants, Microbiota, Mineral biogeochemical cycles, Rhizosphere},
	Pages = {1 - 67},
	Publisher = {Academic Press},
	Series = {Advances in Applied Microbiology},
	Title = {Chapter One - Plant--Microbiota Interactions as a Driver of the Mineral Turnover in the Rhizosphere},
	Url = {http://www.sciencedirect.com/science/article/pii/S0065216416300132},
	Volume = {95},
