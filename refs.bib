	Journal = {Journal of Microbiological Methods},
	Keywords = {, Antimicrobial resistance, Oxford Nanopore ultra-long read sequencing},
	Pages = {138--147},
	Title = {Nanopore ultra-long read sequencing technology for antimicrobial resistance detection in Mannheimia haemolytica},
