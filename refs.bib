	Journal = {New Phytologist},
	Keywords = {core microbiome, evolution, holobiont, interactions, microbiota, plant microbiome, plasticity},
	Number = {4},
	Pages = {1196-1206},
	Title = {The importance of the microbiome of the plant holobiont},
