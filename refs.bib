	Keywords = {Arabidopsis/*genetics; Arabidopsis Proteins/*genetics; Gene Expression Regulation, Plant; Genes, Plant; Models, Genetic; Polymorphism, Single Nucleotide; Probability; *Quantitative Trait Loci},
	L2 = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC3492367/},
	La = {eng},
